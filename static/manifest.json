{
	"manifest_version": 3,
	"name": "Twitch plugins",
	"version": "1.0",
	"description": "Twitch plugins",
	"action": {
		"default_popup": "index.html"
	},
	"permissions": ["activeTab", "storage"],
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": ["https://www.twitch.tv/*"],
			"js": ["content.js"]
		}
	]
}
